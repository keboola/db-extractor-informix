{
  "parameters": {
    "db": {
      "host": "${DB_HOST}",
      "serverName": "${DB_SERVER_NAME}",
      "port": "${DB_PORT}",
      "dbLocale": "${DB_LOCALE_VALUE}",
      "database": "${DB_DATABASE}",
      "user": "${DB_USER}",
      "#password": "${DB_PASSWORD}",
      "ssh": {
        "enabled": true,
        "keys": {
          "#private": "${SSH_PRIVATE_KEY}",
          "public": "${SSH_PUBLIC_KEY}"
        },
        "user": "root",
        "sshHost": "sshproxy"
      }
    },
    "outputTable": "output",
    "table": {
      "tableName": "simple",
      "schema": "${DEFAULT_SCHEMA}"
    },
    "retries": 2
  }
}
